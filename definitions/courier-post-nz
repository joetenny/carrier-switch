SELECT GROUP_CONCAT(ca.ID)
FROM CARRIER_ACCOUNT ca
    LEFT JOIN CLIENT c ON c.ID = ca.C_ID
    JOIN CARRIER_GROUP cg ON (cg.CARRIER_ID = ca.S_ID)
    JOIN CLIENT_GROUP clg ON clg.CLIENT_ID = c.ID
    JOIN CLIENT_GROUP_DATA cgd ON cgd.ID = clg.CLIENT_GROUP_DATA_ID
WHERE ca.LOCKED = 'N' AND cg.GROUP = 'Courierpost'
;